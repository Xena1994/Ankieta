@model IEnumerable<AnkietaProjekt.Models.Test>

@{
    ViewBag.Title = "Index";
}

    <div class="jumbotron w3-container w3-center w3-animate-bottom">
        <h1>Wypełnianie Ankiet!</h1>
    </div>

<p>
    <h3>@Html.ActionLink("Wypełnij Ankietę", "Create")</h3>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.plec)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.plec)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.ID }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
